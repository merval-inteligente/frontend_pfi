{
  "expo": {
    "name": "MERVAL Guide",
    "slug": "merval-guide",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "mervalguide",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "cover",
      "backgroundColor": "#8CD279"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.nicolas.mervalguide",
      "buildNumber": "1",
      "splash": {
        "image": "./assets/images/splash-icon.png",
        "resizeMode": "cover",
        "backgroundColor": "#8CD279",
        "tabletImage": "./assets/images/splash-icon.png"
      },
      "infoPlist": {
        "UIBackgroundModes": [
          "background-fetch"
        ]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#8CD279"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.nicolas.mervalguide"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    }
  }
}
